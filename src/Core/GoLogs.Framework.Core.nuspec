<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>GoLogs.Framework.Core</id>
    <version>0.3.0</version>
    <title>GoLogs.Framework.Core</title>
    <authors>IqbalN</authors>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Core framework for Go-Logs .NET applications.</description>
    <copyright>Go-Logs 2020</copyright>
    <repository type="git" url="https://bitbucket.org/go-logs/gologs.framework.git" branch="master" />
    <dependencies>
      <group targetFramework=".NETCoreApp3.1">
        <dependency id="Microsoft.AspNetCore.Http.Abstractions" version="2.2.0" exclude="Build,Analyzers" />
        <dependency id="StyleCop.Analyzers" version="1.1.118" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="stylecop.json" />
    <file src="GoLogs.stylecop.ruleset" />
    <file src="GoLogs.Framework.Core.props" target="build" />
    <file src="bin/Release/netcoreapp3.1/*.dll" target="lib/netcoreapp3.1" />
    <file src="bin/Release/netcoreapp3.1/*.pdb" target="lib/netcoreapp3.1" />
    <file src="bin/Release/*.snupkg" />
  </files>
</package>